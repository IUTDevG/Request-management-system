{
    "name": "iutdevg/request-management-system",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "request-management-system",
        "iut"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bezhansalleh/filament-language-switch": "^3.1",
        "filament/filament": "^3.2",
        "filament/notifications": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "joaopaulolndev/filament-edit-profile": "*",
        "laravel/framework": "^11.9",
        "laravel/socialite": "^5.15",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.4",
        "resend/resend-laravel": "^0.12.0",
        "rmsramos/activitylog": "^1.0",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-medialibrary": "^11.5",
        "spatie/laravel-permission": "^6.7",
        "ysfkaya/filament-phone-input": "^2.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel-lang/lang": "^15.5",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
